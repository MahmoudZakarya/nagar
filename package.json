  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:api\" \"npm run dev:server\" \"npm run dev:electron\"",
    "dev:api": "ts-node -P tsconfig.server.json server/index.ts",
    "dev:server": "vite",
